#include<stdio.h>
int main()
{
	printf("Name:Anitha\n reg no:192324040 \n");
	int n,a[100],i;
	scanf("%d",&n);
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	printf("even numbers are:");
	for(i=0;i<n;i++)
	{
		if(a[i]%2==0)
		printf("%d\n",a[i]);
	}
	printf("odd numbers are:\n");
	for(i=0;i<n;i++)
	{
		if(a[i]%2==1)
		printf("%d\n",a[i]);
	}
}
