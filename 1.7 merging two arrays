#include<stdio.h>
int main()
{
	printf("Name:Anitha \n reg no:192324040\n");
	int i,n,m,a[100],b[100],c[100],d;
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	scanf("%d",&m);
	for(i=0;i<m;i++)
	scanf("%d",&b[i]);
	for(i=0;i<n;i++)
	c[i]=a[i];
	for(i=0;i<m;i++)
	{
		c[i+n]=b[i];
	}
	d=n+m;
	for(i=0;i<d;i++)
	printf("%d",c[i]);
}
